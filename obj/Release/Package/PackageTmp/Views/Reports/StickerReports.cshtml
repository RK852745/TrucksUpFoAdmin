<script>

    function Getdata() {
        // Show loading GIF
        $("#loadingGif").show();
        var date = $("#reportrange span").text();
        var status = $("#status").val();
        var donby = $("Employee").val();
        date = date.split("-");
        var startdate = date[0];
        var enddate = date[1];

        // Perform an AJAX request to the server to get the Excel file
        $.ajax({
            url: '../Reports/GetVisitsDataAndExportToExcel',
            type: 'post',
            data: {
                startdate: startdate,
                enddate: enddate,
                donby: donby,
                status: status
            },
            xhrFields: {
                responseType: 'blob'
            },
            success: function (data) {
                // Hide loading GIF
                $("#loadingGif").hide();

                // Create a download link and trigger the download
                var a = document.createElement('a');
                var url = window.URL.createObjectURL(data);
                a.href = url;
                a.download = 'Sticker_visits_data.xlsx';
                document.body.appendChild(a);
                a.click();
                window.URL.revokeObjectURL(url);
                document.body.removeChild(a);
            },
            error: function () {
                // Hide loading GIF
                $("#loadingGif").hide();
                alert('An error occurred while exporting data.');
            }
        });
    }
</script>

