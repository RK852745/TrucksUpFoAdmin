        #Datatable1 {
            font-size: 12px;
        }

            #Datatable1 tr th, #Datatable1 tr td {
                padding: 5px 30px 5px 5px;
            }

        table {
            font-size: 13px;
        }

        .numberplate {
            text-align: center;
            letter-spacing: 1.5px;
            color: black;
            background: gold;
            border-radius: 5px;
            padding: 2px;
        }

        .innerplate {
            padding: 1px 3px;
            border-radius: 5px;
            border: 1px solid gray;
            font-weight: 450;
            text-transform: uppercase;
        }

        .badge {
            font-weight: 100 !important;
        }

        .verif__btn {
            padding: 5px;
            text-align: center;
            background: #e8e8e8;
            border-radius: 6px;
            cursor: pointer;
        }

        .approvebtn:hover {
            background: #a6ffa9;
            cursor: pointer;
        }

        .rejectbtn:hover {
            background: #ffa6a6;
            cursor: pointer;
        }

        .Abtnactive {
            background: #a6ffa9;
            cursor: pointer;
        }

        .Rbtnactive {
            background: #ffa6a6;
            cursor: pointer;
        }

        .swal2-container {
            z-index: 999999999999999 !important;
        }
        .countercards {
            border: 2px solid #0000FF; /* Blue border color */
        }
    </style>
}
